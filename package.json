{
    "name": "curiculum",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://kostiask1.github.io/curiculum/",
    "dependencies": {
        "firebase": "^8.10.0",
        "gh-pages": "^3.2.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^5.0.0",
        "react-scroll": "^1.8.2",
        "react-transition-group": "^4.4.2",
        "sass": "^1.34.1"
    },
    "scripts": {
        "watch": "react-scripts start --host 0.0.0.0",
        "build": "react-scripts build",
        "deploy": "firebase deploy",
        "serve": "serve -s build",
        "build-serve": "react-scripts build && serve -s build",
        "deployGit": "gh-pages -d build",
        "release": "react-scripts build && firebase deploy && gh-pages -d build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
